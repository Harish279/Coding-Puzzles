#include <iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    int leader=a[n-1];
    cout<<a[n-1]<<" ";
    for(int i=n-2;i>=0;i--)
    {
        if(a[i]>=leader)
           {
            leader=a[i];
            cout<<leader<<" ";
           }
    }
return 0;
}
